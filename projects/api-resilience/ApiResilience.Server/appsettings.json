{
  "Logging": {
    "ColorConsole": {
      "LogLevelToColorMap": {
        "Trace": "DarkGray",
        "Debug": "Gray",
        "Information": "DarkGreen",
        "Warning": "Yellow",
        "Error": "Red",
        "Critical": "DarkRed"
      },
      "SimplifiedOutput": true
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "None",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Watch.BrowserRefresh.BrowserRefreshMiddleware": "None",
      "Microsoft.AspNetCore.Watch.BrowserRefresh.BlazorWasmHotReloadMiddleware": "None",
      "Microsoft.AspNetCore.Watch.BrowserRefresh.BrowserScriptMiddleware": "None",
      "Microsoft.AspNetCore.Server.Kestrel": "Error",
      "ApiResilience.Server.GlobalExceptionHandler": "Trace"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://localhost:3000"
      }
    }
  }
}
